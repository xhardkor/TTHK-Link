<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TTHK_Link.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:TTHK_Link.Pages"
    xmlns:local="clr-namespace:TTHK_Link"
    Title="TTHK_Link">

    <ShellContent
        Route="login"
        ContentTemplate="{DataTemplate pages:LoginPage}" />

    <ShellContent
        Route="groups"
        ContentTemplate="{DataTemplate pages:CoursePage}" />

    <ShellContent
        Route="chat"
        ContentTemplate="{DataTemplate pages:ChatPage}" />

</Shell>